using System;

class Program
{
    static void Main()
    {
        int[] numbers = { 1, 3, 5, 6, 7, 8 }; // Исходный массив
        int temp;

        // Вывод исходного массива
        Console.Write("Исходный массив: ");
        foreach (int number in numbers)
        {
            Console.Write(number + " ");
        }

        // Добавлен пропущенный закрывающий фигурный скобка
        Console.WriteLine();
    }
}
